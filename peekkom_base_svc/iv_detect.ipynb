    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# 코드\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 1. 데이터 다운로드 및 압축 해제\\n\",\n",
    "    \"!wget -O Aquarium_Data.zip https://app.roboflow.com/ds/54KhujduUB?key=4e4yhwdsYR\\n\",\n",
    "    \"import zipfile, os\\n\",\n",
    "    \"\\n\",\n",
    "    \"with zipfile.ZipFile('./Aquarium_Data.zip') as zf:\\n\",\n",
    "    \"    zf.extractall('./Aquarium_Data/')\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 2. YAML 생성\\n\",\n",
    "    \"!pip install -q PyYAML\\n\",\n",
    "    \"import yaml\\n\",\n",
    "    \"\\n\",\n",
    "    \"data = {\\n\",\n",
    "    \"    'train': '/content/Aquarium_Data/train/images',\\n\",\n",
    "    \"    'val': '/content/Aquarium_Data/valid/images',\\n\",\n",
    "    \"    'test': '/content/Aquarium_Data/test/images',\\n\",\n",
    "    \"    'names': ['iv_bag', 'saline_level'],\\n\",\n",
    "    \"    'nc': 2\\n\",\n",
    "    \"}\\n\",\n",
    "    \"\\n\",\n",
    "    \"yaml_path = '/content/Aquarium_Data/Aquarium_Data.yaml'\\n\",\n",
    "    \"with open(yaml_path, 'w') as f:\\n\",\n",
    "    \"    yaml.dump(data, f)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 3. YOLOv8 설치 및 환경 확인\\n\",\n",
    "    \"!pip install -q ultralytics\\n\",\n",
    "    \"import ultralytics\\n\",\n",
    "    \"ultralytics.checks()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 4. 모델 불러오기 및 학습\\n\",\n",
    "    \"from ultralytics import YOLO\\n\",\n",
    "    \"model = YOLO('yolov8n.pt')  # 또는 yolov8s.pt (더 큰 모델)\\n\",\n",
    "    \"model.train(data=yaml_path, epochs=100, patience=20, batch=8, imgsz=640)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 5. 검증 데이터셋 평가\\n\",\n",
    "    \"metrics = model.val(data=yaml_path)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 6. 성능 지표 출력\\n\",\n",
    "    \"mAP_5 = metrics.results_dict['metrics/mAP50(B)']\\n\",\n",
    "    \"mAP_5over = metrics.results_dict['metrics/mAP50-95(B)']\\n\",\n",
    "    \"precision = metrics.results_dict['metrics/precision(B)']\\n\",\n",
    "    \"recall = metrics.results_dict['metrics/recall(B)']\\n\",\n",
    "    \"fitness = metrics.results_dict['fitness']\\n\",\n",
    "    \"f1_score = 2 * (precision * recall) / (precision + recall) if precision + recall > 0 else 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"print(f\\\"mAP@0.5: {mAP_5:.4f}\\\")\\n\",\n",
    "    \"print(f\\\"mAP@0.5:0.95: {mAP_5over:.4f}\\\")\\n\",\n",
    "    \"print(f\\\"Precision: {precision:.4f}\\\")\\n\",\n",
    "    \"print(f\\\"Recall: {recall:.4f}\\\")\\n\",\n",
    "    \"print(f\\\"F1 Score: {f1_score:.4f}\\\")\\n\",\n",
    "    \"print(f\\\"Fitness: {fitness:.4f}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 7. 성능 그래프 시각화\\n\",\n",
    "    \"import matplotlib.pyplot as plt\\n\",\n",
    "    \"\\n\",\n",
    "    \"metrics_dict = {\\n\",\n",
    "    \"    \\\"mAP@0.5\\\": mAP_5,\\n\",\n",
    "    \"    \\\"mAP@0.5:0.95\\\": mAP_5over,\\n\",\n",
    "    \"    \\\"Precision\\\": precision,\\n\",\n",
    "    \"    \\\"Recall\\\": recall,\\n\",\n",
    "    \"    \\\"F1 Score\\\": f1_score,\\n\",\n",
    "    \"}\\n\",\n",
    "    \"\\n\",\n",
    "    \"plt.figure(figsize=(8, 5))\\n\",\n",
    "    \"plt.bar(metrics_dict.keys(), metrics_dict.values(), color='skyblue')\\n\",\n",
    "    \"plt.title(\\\"YOLOv8 Evaluation Metrics\\\")\\n\",\n",
    "    \"plt.ylim(0, 1.0)\\n\",\n",
    "    \"plt.grid(axis='y', linestyle='--')\\n\",\n",
    "    \"plt.show()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 8. 테스트 이미지 예측 및 결과 저장\\n\",\n",
    "    \"from glob import glob\\n\",\n",
    "    \"test_images = sorted(glob('/content/Aquarium_Data/test/images/*'))\\n\",\n",
    "    \"results = model.predict(source='/content/Aquarium_Data/test/images', save=True)\\n\",\n",
    "    \"\\n\",\n",
    "    \"from PIL import Image\\n\",\n",
    "    \"from IPython.display import display\\n\",\n",
    "    \"import os\\n\",\n",
    "    \"\\n\",\n",
    "    \"image_dir = 'runs/detect/train3'\\n\",\n",
    "    \"\\n\",\n",
    "    \"for filename in os.listdir(image_dir):\\n\",\n",
    "    \"    if filename.lower().endswith(('.jpg', '.png')):\\n\",\n",
    "    \"        image_path = os.path.join(image_dir, filename)\\n\",\n",
    "    \"        print(f\\\"Displaying: {filename}\\\")\\n\",\n",
    "    \"        display(Image.open(image_path))\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 10. 예측 클래스별 개수 출력\\n\",\n",
    "    \"from collections import Counter\\n\",\n",
    "    \"\\n\",\n",
    "    \"for result in results:\\n\",\n",
    "    \"    class_ids = [int(cls) for cls in result.boxes.cls]\\n\",\n",
    "    \"    count = Counter(class_ids)\\n\",\n",
    "    \"    print(f\\\"\\\\nClass counts: {dict(count)}\\\")\\n\",\n",
    "    \"    for idx, class_id in enumerate(class_ids):\\n\",\n",
    "    \"        class_name = result.names[class_id]\\n\",\n",
    "    \"        conf = float(result.boxes.conf[idx])\\n\",\n",
    "    \"        print(f'Class {class_id:>2} ({class_name:<13}) | Confidence: {conf:.2f}')\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 11. 모델 저장\\n\",\n",
    "    \"model.save('trained_yolov8n.pt')\\n\"\n",
    "   ]\n",
    "  }\n",
    " ],\n",
